{
  "name": "consul-cli-js",
  "version": "2.0.4",
  "description": "A node.js wrapper for the consul CLI command",
  "license": "MIT",
  "author": {
    "name": "Matthias Ludwig",
    "email": "mludwig@quobject.io"
  },
  "main": "./dist/main.js",
  "scripts": {
    "clean": "npx del dist",
    "lint": "npx tslint --project .",
    "compile": "npx tsc",
    "build": "npm run lint && npm run clean && npm run compile",
    "test": "npm run build && npx jasmine",
    "app": "npm run compile && node \"./dist/main.js\" ",
    "crunch": "chokidar \"src/**/*.*\" -c \"npm run test\" --silent=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quobject/consul-cli-js.git"
  },
  "keywords": [
    "consul"
  ],
  "devDependencies": {
    "@types/jasmine": "3.3.8",
    "@types/del": "^3.0.1",
    "del-cli": "1.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "typescript": "3.2.4",
    "del": "^3.0.0",
    "jasmine": "3.3.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-ts-console-reporter": "^3.1.1",
    "boscode": "0.0.7",
    "chokidar-cli": "^1.2.1",
    "npx": "^10.2.0"
  },
  "dependencies": {
    "cli-table-2-json": "^1.0.11",
    "lodash": "^4.17.11",
    "nodeify-ts": "^1.0.6"
  }
}
